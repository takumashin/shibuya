<% include navbar.ejs %>
        <div class="page-content mdl-grid">
            <% if (success) { %>
                <div class="mdl-cell mdl-cell--12-col cell-color demo-card-square mdl-card mdl-shadow--2dp card-width-fix">
                    <div class="mdl-card__title mdl-card--expand">
                        <h2 class="mdl-card__title-text"><%= success %></h2>
                    </div>
                </div>
            <% } %>
            <% if (error) { %>
                <div class="mdl-cell mdl-cell--12-col cell-color demo-card-square mdl-card mdl-shadow--2dp card-width-fix">
                    <div class="mdl-card__title mdl-card--expand">
                        <h2 class="mdl-card__title-text"><%= error %></h2>
                    </div>
                </div>
            <% } %>



            <% posts.forEach(function (post, index) { %>
            <div class="mdl-card mdl-cell mdl-cell--12-col">
                <div class="mdl-card__media mdl-color-text--grey-50" style="background: url(<%= post.image %>)">
                    <h3><a href="javascript:;"><%= post.title %></a></h3>
                </div>
                <div class="mdl-color-text--grey-600 mdl-card__supporting-text">
                    <%- post.post %>
                </div>
                <div class="mdl-card__supporting-text meta mdl-color-text--grey-600">
                    <div class="minilogo"></div>
                    <div>
                        <strong><%= post.name %></strong>
                        <span><%= post.time.minute %></span>
                    </div>
                </div>
            </div>
            <% }) %>
        </div>
<% include footer.ejs %>